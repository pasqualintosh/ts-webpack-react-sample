  "description": "simple react app with webpack",
  "main": "index.js",
  "jest": {
    "verbose": true,
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/src/__mocks__/styleMock.js",
